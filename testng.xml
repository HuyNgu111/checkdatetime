<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="DateTimeChecker LambdaTest Suite" parallel="tests" thread-count="5">
    
    <test name="Chrome on Windows 10">
        <parameter name="browser" value="Chrome"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="Windows 10"/>
        <classes>
            <class name="selenium.LambdaTestRunner"/>
        </classes>
    </test>
    
    <test name="Firefox on Windows 10">
        <parameter name="browser" value="Firefox"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="Windows 10"/>
        <classes>
            <class name="selenium.LambdaTestRunner"/>
        </classes>
    </test>
    
    <test name="Edge on Windows 10">
        <parameter name="browser" value="MicrosoftEdge"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="Windows 10"/>
        <classes>
            <class name="selenium.LambdaTestRunner"/>
        </classes>
    </test>
    
    <test name="Safari on macOS Monterey">
        <parameter name="browser" value="Safari"/>
        <parameter name="version" value="15"/>
        <parameter name="platform" value="macOS Monterey"/>
        <classes>
            <class name="selenium.LambdaTestRunner"/>
        </classes>
    </test>
    
    <test name="Chrome on macOS">
        <parameter name="browser" value="Chrome"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="macOS Monterey"/>
        <classes>
            <class name="selenium.LambdaTestRunner"/>
        </classes>
    </test>
    
</suite>
